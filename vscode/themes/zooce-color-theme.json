		{
			"scope": "keyword.control.flow",
			"settings": {
				"foreground": "#A37ACC"
			}
		},
		{
			"scope": "keyword.control.import",
			"settings": {
				"foreground": "#A37ACC"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#A37ACC"
			}
		},
		{
			"scope": "keyword.other",
