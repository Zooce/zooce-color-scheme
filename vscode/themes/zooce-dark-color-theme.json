{
    "name": "Zooce",
    "colors": {
        "editor.foreground": "#F2F2F2",
        "editor.background": "#191919",
        "editorWhitespace.foreground": "#8A8A8A",
        "editorCursor.foreground": "#F2F2F2",
        "editor.lineHighlightBackground": "#000000",
        "editor.selectionBackground": "#BF353F",
        "editor.selectionHighlightBorder": "#85DB18",
        "editor.inactiveSelectionBackground": "#85DB18",
        "editorIndentGuide.background": "#222218",
        "editorGutter.background": "#000000",
        "editorBracketMatch.border": "#85DB18",
        "editorOverviewRuler.bracketMatchForeground": "#85DB18",
        "editor.findMatchBorder": "#85DB18"
    },
    "tokenColors": [
        //===--- PUNCTUATION ---===//
        {
            "scope": "punctuation.definition.arguments",
            "settings": {
                "foreground": "#F2F2F2"
            }
        },
        {
            "scope": "punctuation.section.arguments",
            "settings": {
                "foreground": "#F2F2F2"
            }
        },
        {
            "scope": "punctuation.separator",
            "settings": {
                "foreground": "#F2F2F2"
            }
        },
        {
            "scope": "punctuation.definition.logical-expression",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        //===--- COMMENT ---===//
        {
            "scope": "comment.line",
            "settings": {
                "foreground": "#E8388F"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#8A8A8A"
            }
        },
        //===--- ENTITY ---===//
        {
            "scope": "entity.name.function.decorator",
            "settings": {
                "foreground": "#8A8A8A"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        {
            "scope": "entity.name.type",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        {
            "scope": "entity.name.section",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        //===--- STRING ---===//
        {
            "scope": "string.quoted.docstring",
            "settings": {
                "foreground": "#8A8A8A"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#8EB1E5"
            }
        },
        {
            "scope": "string.quoted.double.shell variable.other.normal.shell",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        //===--- SOURCE ---===//
        {
            "scope": "source.ini",
            "settings": {
                "foreground": "#E8388F"
            }
        },
        //===--- CONSTANT ---===//
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        {
            "scope": "constant.character",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        {
            "scope": "constant.other",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        //===--- VARIABLE ---===//
        {
            "scope": "variable.other.bracket",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        {
            "scope": "variable.language",
            "settings": {
                "foreground": "#E8388F"
            }
        },
        {
            "scope": "variable.other.special",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        {
            "scope": "variable.other.positional",
            "settings": {
                "foreground": "#D0FF00"
            }
        },
        {
            "scope": "variable.other.normal.shell",
            "settings": {
                "foreground": "#528EE5"
            }
        },
        {
            "scope": "variable.parameter.function.language.special",
            "settings": {
                "foreground": "#E8388F"
            }
        },
        //===--- STORAGE ---===//
        {
            "scope": "storage",
            "settings": {
                "foreground": "#528EE5"
            }
        },
        //===--- SUPPORT ---===//
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#528EE5"
            }
        },
        {
            "scope": "support.function.builtin",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        //===--- KEYWORD ---===//
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "keyword.control.directive",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "keyword.control.flow",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "keyword.control.import",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#9762CC"
            }
        },
        {
            "scope": "keyword.control.cpp",
            "settings": {
                "foreground": "#20B2AA"
            }
        },
        //===--- META ---===//
        {
            "scope": "meta.function-call.arguments",
            "settings": {
                "foreground": "#F2F2F2"
            }
        },
        {
            "scope": "meta.function-call.python",
            "settings": {
                "foreground": "#20B2AA"
            }
        }
    ]
}
